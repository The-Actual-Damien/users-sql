{
  "name": "@reactive-underground/users-sql",
  "version": "0.0.2",
  "description": "Sql Repository implementations for reactive-underground/users",
  "author": "Artem Ilinykh <devsinglesly@gmail.com>",
  "private": false,
  "license": "UNLICENSED",
  "keywords": [
    "users",
    "login",
    "crud",
    "sql",
    "repository",
    "reactive-underground"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "main": "dist/index.js",
  "files": [
    "dist/**/*"
  ],
  "directories": {
    "man": "dist"
  },
  "repository": {
    "url": "https://github.com/reactive-underground/users-sql",
    "type": "git"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "prepublish": "build",
    "build": "tsc --project tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
    "@reactive-underground/users": "0.0.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@types/jest": "25.2.3",
    "@types/node": "^13.9.1",
    "@typescript-eslint/eslint-plugin": "3.0.2",
    "@typescript-eslint/parser": "3.0.2",
    "eslint": "7.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "26.0.1",
    "pg": "8.3.3",
    "prettier": "^1.19.1",
    "sqlite3": "^5.0.0",
    "ts-jest": "26.1.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typeorm": "^0.2.26",
    "typescript": "^3.7.4"
  },
  "peerDependencies": {
    "typeorm": "^0.2.26"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
